{
  "plugins": ["react-hooks"],
  "extends": ["airbnb", "airbnb/hooks", "prettier"],
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-filename-extension": 0,
    "react/state-in-constructor": 0,
    "import/prefer-default-export": 0,
    "import/no-unresolved": 0,
    "react/jsx-props-no-spreading": 0,
    "import/no-cycle": 0,
    "no-restricted-syntax": 0
  },
  "globals": {
    "document": true,
    "window": true
  }
}
